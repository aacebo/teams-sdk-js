{
  "name": "teams-sdk-js",
  "private": true,
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.8.1",
  "workspaces": [
    "apps/*",
    "packages/*",
    "samples/*"
  ],
  "scripts": {
    "clean": "turbo clean",
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "test": "turbo test",
    "fmt": "prettier --write \"**/*.{js,ts,tsx,md,json}\""
  },
  "devDependencies": {
    "@changesets/cli": "latest",
    "prettier": "^3.2.5",
    "turbo": "latest",
    "typescript": "^5.4.5"
  }
}
